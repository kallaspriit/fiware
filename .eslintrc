{
	"parser": "babel-eslint",
	"settings": {
		"ecmascript": 6
	},
	"plugins": [],
	"ecmaFeatures": {
		"arrowFunctions": true,
		"destructuring": true,
		"modules": true,
		"blockBindings": true,
		"forOf": true,
		"jsx": true
	},
	"rules": {

		// possible errors
		"no-console": 0,
		"no-debugger": 2,
		"no-cond-assign": 2,
		"no-constant-condition": 2,
		"no-dupe-args": 2,
		"no-dupe-keys": 2,
		"no-duplicate-case": 2,
		"no-ex-assign": 2,
		"no-empty": 2,
		"no-extra-boolean-cast": 2,
		"no-extra-semi": 2,
		"no-func-assign": 2,
		"no-inner-declarations": 2,
		"no-invalid-regexp": 2,
		"no-irregular-whitespace": 2,
		"no-negated-in-lhs": 2,
		"no-obj-calls": 2,
		"no-regex-spaces": 2,
		"no-sparse-arrays": 2,
		"no-unreachable": 2,
		"no-control-regex": 2,
		"no-empty-character-class": 2,
		"no-unexpected-multiline": 2,
		"comma-dangle": 2,
		"use-isnan": 2,
		"valid-typeof": 2,
		"valid-jsdoc": [2, {
			"requireReturn": false,
			"requireParamDescription": false,
			"requireReturnDescription": false
		}],

		// best practices
		"accessor-pairs": 2,
		"block-scoped-var": 2,
		"consistent-return": 2,
		"curly": 2,
		"default-case": 2,
		"dot-notation": 2,
		"dot-location": [2, "property"],
		"eqeqeq": 2,
		"guard-for-in": 2,
		"no-alert": 2,
		"no-caller": 2,
		"no-case-declarations": 2,
		"no-div-regex": 2,
		"no-else-return": 0,
		"no-eval": 2,
		"no-labels": 2,
		"no-empty-pattern": 2,
		"no-eq-null": 2,
		"no-extend-native": 2,
		"no-extra-bind": 2,
		"no-fallthrough": 2,
		"no-floating-decimal": 2,
		"no-implied-eval": 2,
		"no-implicit-coercion": 2,
		"no-iterator": 2,
		"no-invalid-this": 2,
		"no-lone-blocks": 2,
		"no-loop-func": 2,
		"no-multi-spaces": 2,
		"no-multi-str": 2,
		"no-native-reassign": 2,
		"no-new": 2,
		"no-new-func": 2,
		"no-new-wrappers": 2,
		"no-octal": 2,
		"no-octal-escape": 2,
		"no-param-reassign": [0, {"props": true}],
		"no-process-env": 2,
		"no-proto": 2,
		"no-redeclare": 2,
		"no-return-assign": 2,
		"no-script-url": 2,
		"no-self-compare": 2,
		"no-sequences": 2,
		"no-throw-literal": 2,
		"no-unused-expressions": 2,
		"no-useless-call": 2,
		"no-useless-concat": 2,
		"no-void": 0,
		"no-warning-comments": [0, {"terms": ["todo"], "location": "anywhere"}],
		"no-with": 2,
		"wrap-iife": [2, "inside"],
		"yoda": 2,
		"radix": 2,
		"strict": 2,
		"complexity": [1, 10],

		// "no-magic-numbers": 2,

		// variables
		"no-catch-shadow": 2,
		"no-delete-var": 2,
		"no-label-var": 2,
		"no-shadow": 2,
		"no-shadow-restricted-names": 2,
		"no-undef": 2,
		"no-undef-init": 2,
		"no-undefined": 0,
		"no-unused-vars": [2, {"vars": "all", "args": "after-used"}],
		"no-use-before-define": 0,

		// nodejs
		"callback-return": 2,
		"global-require": 0,
		"no-mixed-requires": 2,
		"no-new-require": 2,
		"no-path-concat": 2,
		"no-process-exit": 2,

		// stylistic issues
		"array-bracket-spacing": [2, "never"],
		"block-spacing": [2, "always"],
		"computed-property-spacing": [2, "never"],
		"indent": [2, "tab", {"SwitchCase": 1}],
		"brace-style": [2, "1tbs"],
		"camelcase": 2,
		"comma-spacing": [2, {"before": false, "after": true}],
		"comma-style": [2, "last"],
		"consistent-this": [2, "self"],
		"eol-last": 0,
		"func-style": [0, "declaration"],
		"key-spacing": [2, {
			"beforeColon": false,
			"afterColon": true
		}],
		"new-cap": [2, {
			"capIsNewExceptions": ["Router"]
		}],
		"new-parens": 2,
		"newline-after-var": 2,
		"no-array-constructor": 2,
		"no-continue": 0,
		"no-lonely-if": 2,
		"no-mixed-spaces-and-tabs": [2, "smart-tabs"],
		"no-multiple-empty-lines": 2,
		"no-nested-ternary": 2,
		"no-negated-condition": 2,
		"no-new-object": 2,
		"no-spaced-func": 2,
		"no-extra-parens": [2, "functions"],
		"operator-assignment": [2, "always"],
		"operator-linebreak": [2, "before"],
		"quote-props": 0,
		"quotes": [2, "single"],
		"semi": [2, "always"],
		"semi-spacing": [2, {"before": false, "after": true}],
		"keyword-spacing": 2,
		"space-before-blocks": [2, "always"],
		"space-before-function-paren": [2, "never"],
		"object-curly-spacing": [2, "always"],
		"space-in-parens": [2, "never"],
		"space-infix-ops": 2,
		"space-unary-ops": [2, { "words": true, "nonwords": false }],
		"spaced-comment": 2,
		"wrap-regex": 2,
		"max-len": [2, 120, 4],
		"no-underscore-dangle": 0,
		"no-trailing-spaces": 0,
		"jsx-quotes": 2,
		"one-var": [2, "never"],

		// ES6
		"no-var": 2,
		"prefer-const": 2
	},
	"env": {
		"browser": true,
		"node": true,
		"jasmine": true
	},

	"globals": {
		// jsx control statements
		"If": true,
		"Else": true,

		// debug helpers
		"dbg": true,

		// node native promise
		"Promise": true
	},

	"ext": [".js", ".jsx"]
}